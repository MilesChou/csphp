{
    "name": "mileschou/csphp",
    "license": "MIT",
    "description": "The builder and parser for PHP",
    "keywords": [
        "csp"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "psy/psysh": "^0.9.9",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/console": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Csphp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app",
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.5"
        }
    }
}
