{
    "name": "mileschou/csphp",
    "license": "MIT",
    "description": "The builder and parser for PHP",
    "keywords": [
        "csp"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": ">=4.8 <6",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Csphp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.5"
        }
    }
}
